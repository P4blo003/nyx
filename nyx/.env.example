# ==============================
# GRAFANA
# ==============================

GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=admin


# ==============================
# QDRANT
# ==============================

# API key used by clients to authenticate against the Qdrant service.
# Change this value in production and keep it secret.
QDRANT_API_KEY=change-me
# Enable or disable TLS support for Qdrant.
# - true: TLS enabled (HTTPS / secure gRPC, requires certificates).
# - false: TLS disabled (plain HTTP, gRPC, recommended only for internal networks).
QDRANT_ENABLE_TLS=false